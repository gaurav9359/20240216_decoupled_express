{
    "type": "object",
    "properties": {
      "id": {
        "type": "number",
        "uniqueItems": true
      },
      "order_date": {
        "type": "string",
        "format": "date-time"
      },
      "order_address": {
        "type": "string"
      },
      "order_pId": {
        "type": "number"
      },
      "order_status": {
        "type": "string",
        "enum": ["dispatched", "cancelled", "delivered"]
      },
      "total_cost": {
        "type": "number"
      }
    },
    "required": ["id", "order_date", "order_address", "order_pId", "order_status", "total_cost"]
  }
  